{
  "name": "fushie-fish",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-eslint": "^7.1.1",
    "concurrently": "3.0.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.11.0",
    "onchange": "^3.2.1",
    "postcss": "^5.0.16",
    "postcss-cli": "^2.5.1",
    "react-scripts": "0.6.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "eslint-config-airbnb": "^14.1.0",
    "history": "4.2.0",
    "re-base": "2.2.0",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "4.0.0-alpha.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "watch": "concurrently --names \"webpack, postcss\" --prefix name \"npm run start\" \"npm run postcss\" \"npm run watch-css\"",
    "postcss": "node-sass --include-path scss -w ./src/css/style.scss ./src/css/style.css",
    "watch-css": "nodemon -e scss -x \"npm run autoprefix\"",
    "autoprefix": "postcss -u autoprefixer -r ./src/css/style.css"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/.eslintrc"
  }
}
